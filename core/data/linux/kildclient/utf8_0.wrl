  <timers>
    <timer name="check_idle" interval="60" count="-1" enabled="1" temporary="0">/check_idle 60,&quot;idle_action&quot;</timer>
  </timers>
  <hooksv2>
  </hooksv2>
  <protocols>
    <mccp behavior="0"/>
  </protocols>
</kcworld>
