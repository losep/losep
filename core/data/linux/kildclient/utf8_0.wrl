<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcworld SYSTEM "kcworld.dtd">

<kcworld>
  <name>utf8</name>
  <host tls="0">localhost</host>
  <port>6666</port>
  <charset>UTF-8</charset>
  <auto-logon style="0">
  </auto-logon>
  <display>
    <terminalfont>Monospace 10</terminalfont>
    <entryfont>Monospace 10</entryfont>
    <usebold>1</usebold>
    <wrap enabled="1"/>
    <statusfont>Sans 8</statusfont>
    <statusbar ctime="0" itime="0" itime-behavior="0"/>
    <defforecolor>0 48830 48830 48830</defforecolor>
    <defbackcolor>0 0 0 0</defbackcolor>
    <defboldcolor>0 65535 65535 65535</defboldcolor>
    <defboldbackcolor>0 24415 24415 24415</defboldbackcolor>
    <ansicolor>0 0 0 0</ansicolor>
    <ansicolor>1 35723 0 0</ansicolor>
    <ansicolor>2 0 29555 0</ansicolor>
    <ansicolor>3 35723 26985 5140</ansicolor>
    <ansicolor>4 0 0 35723</ansicolor>
    <ansicolor>5 35723 0 35723</ansicolor>
    <ansicolor>6 0 35723 35723</ansicolor>
    <ansicolor>7 48830 48830 48830</ansicolor>
    <ansicolor>8 24415 24415 24415</ansicolor>
    <ansicolor>9 65535 0 0</ansicolor>
    <ansicolor>10 0 65535 0</ansicolor>
    <ansicolor>11 65535 65535 0</ansicolor>
    <ansicolor>12 0 0 65535</ansicolor>
    <ansicolor>13 65535 0 65535</ansicolor>
    <ansicolor>14 0 65535 65535</ansicolor>
    <ansicolor>15 65535 65535 65535</ansicolor>
  </display>
  <repeat-commands enabled="0" />
  <command-echo enabled="1" />
  <store-commands enabled="1" />
  <command-history size="50" />
  <auto-completion enabled="1" minprefix="3" />
  <command-separator separator="%;" />
  <input-lines size="1" save="2" />
  <input-spell active="1" />
  <flood-prevention enabled="1" max-repeat="20">look</flood-prevention>
  <scroll on-output="0" lines="2000" />
  <log autostart="0" addtime="1">
    <logtimeformat>%c&gt; </logtimeformat>
  </log>
  <confirm-delete confirm="1" />
  <plugin-items show="0" />
  <scriptfile>/myplace/appdata/kildclient/local.pl</scriptfile>
  <triggers>
    <trigger name="login" enabled="1" keepexecuting="0" rewriter="0" ignorecase="0">
      <pattern>Do you want to use BIG5 code</pattern>
      <action>n</action>
      <highlight enabled="0" target="0" fg="-1" bg="-1" italics="-1" strike="-1" underline="-1"/>
    </trigger>
    <trigger name="study" enabled="0" keepexecuting="0" rewriter="0" ignorecase="0">
      <pattern>你听了老学士的指导，似乎有些心得。</pattern>
      <action>learn shi literate</action>
      <highlight enabled="0" target="0" fg="-1" bg="-1" italics="-1" strike="-1" underline="-1"/>
    </trigger>
    <trigger name="study" enabled="0" keepexecuting="0" rewriter="0" ignorecase="0">
      <pattern>你今天太累了，结果什么也没有学到。</pattern>
      <action>w%;w%;hp%;skills%;sleep</action>
      <highlight enabled="0" target="0" fg="-1" bg="-1" italics="-1" strike="-1" underline="-1"/>
    </trigger>
    <trigger name="study" enabled="0" keepexecuting="0" rewriter="0" ignorecase="0">
      <pattern>你一觉醒来，只觉精力充沛。该活动一下了。</pattern>
      <action>e%;e%;learn shi literate</action>
      <highlight enabled="0" target="0" fg="-1" bg="-1" italics="-1" strike="-1" underline="-1"/>
    </trigger>
    <trigger name="study" enabled="0" keepexecuting="0" rewriter="0" ignorecase="0">
      <pattern>你的潜能不够，没有办法再成长了。</pattern>
      <action>study_done</action>
      <highlight enabled="0" target="0" fg="-1" bg="-1" italics="-1" strike="-1" underline="-1"/>
    </trigger>
  </triggers>
  <aliases>
    <alias name="idle_action" perleval="0" enabled="1" ignorecase="0">
      <pattern>idle_action</pattern>
      <substitution>hp</substitution>
    </alias>
    <alias name="study_done" perleval="0" enabled="1" ignorecase="0">
      <pattern>^study_done$</pattern>
      <substitution>/\$world-&gt;distrigger(&apos;study&apos;)%;skills</substitution>
    </alias>
    <alias name="ds" perleval="0" enabled="1" ignorecase="0">
      <pattern>^#(\d+)\s+(.*)</pattern>
      <substitution>/repeat $1,&quot;$2&quot;</substitution>
    </alias>
    <alias name="qss$" perleval="0" enabled="1" ignorecase="0">
      <pattern>^#qs\s+(.+)$</pattern>
      <substitution>/qs &quot;$1&quot;</substitution>
    </alias>
    <alias name="Common::repeat" perleval="0" enabled="1" ignorecase="0">
      <pattern>^#(\d+)\s+(.*)</pattern>
      <substitution>/repeat $1,&quot;$2&quot;</substitution>
    </alias>
    <alias name="Common::quicksend" perleval="0" enabled="1" ignorecase="0">
      <pattern>^#qs\s+(.+)$</pattern>
      <substitution>/qs &quot;$1&quot;</substitution>
    </alias>
    <alias name="Common::loot" perleval="0" enabled="1" ignorecase="0">
      <pattern>^loot(\s*|\s+[^\s]+\s*)$</pattern>
      <substitution>/loot(&quot;$1&quot;)</substitution>
    </alias>
  </aliases>
  <macros>
    <macro name="MOD2KP_1" enabled="1">
      <key>&lt;Mod2&gt;KP_1</key>
      <action>southwest</action>
    </macro>
