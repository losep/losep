    <alias name="start_walk" perleval="0" enabled="1" ignorecase="0">
      <pattern>^start_walk$</pattern>
      <substitution>/\$world-&gt;enatrigger(&quot;kill&quot;);my \$dd=random_exit(\$str_exits,\$x_exits);if(\$dd) {\$world-&gt;send(\$dd);delay(1,&apos;new_place&apos;);} else {delay(1,&apos;lock_here&apos;)}</substitution>
    </alias>
    <alias name="lock_here" perleval="1" enabled="1" ignorecase="0">
      <pattern>^lock_here$</pattern>
      <substitution>($direction ? &quot;set_direction&quot; : &quot;set_direction 1&quot;) . &quot;%;fight_done&quot;</substitution>
    </alias>
    <alias name="drop_valueless" perleval="0" enabled="1" ignorecase="0">
      <pattern>^drop_valueless$</pattern>
      <substitution>/\$world-&gt;sendecho(map(&quot;drop \$_&quot;,\@valueless,&quot;\$weapon 2&quot;))</substitution>
    </alias>
    <alias name="death" perleval="1" enabled="1" ignorecase="0">
      <pattern>^death$</pattern>
      <substitution>$world-&gt;distrigger(&apos;fighting&apos;);$world-&gt;distimer(&apos;fighting&apos;);&quot;lock_here&quot;</substitution>
    </alias>
    <alias name="do_perform" perleval="0" enabled="1" ignorecase="0">
      <pattern>^do_perform$</pattern>
      <substitution>/random_send(\@performs);random_send(\@performs1);random_send(\@performs2)</substitution>
    </alias>
    <alias name="wield_weapon" perleval="1" enabled="1" ignorecase="0">
      <pattern>^wield_weapon\s*(.*)\s*$</pattern>
      <substitution>if($1) {our $weapon=$1;};&quot;wield $weapon&quot;</substitution>
    </alias>
    <alias name="unwield_weapon" perleval="1" enabled="1" ignorecase="0">
      <pattern>^unwield_weapon$</pattern>
      <substitution>&quot;unwield $weapon&quot;</substitution>
    </alias>
    <alias name="loot_start" perleval="1" enabled="1" ignorecase="0">
      <pattern>^loot_start$</pattern>
      <substitution>$world-&gt;enatrigger(&quot;loot&quot;);&quot;loot_room&quot;</substitution>
    </alias>
    <alias name="loot_corpse" perleval="1" enabled="1" ignorecase="0">
      <pattern>^loot_corpse$</pattern>
      <substitution>$world-&gt;enatrigger(&quot;loot_corpse&quot;);$loot_target++;$loot_env=&quot;from corpse $loot_target&quot;;&quot;look corpse $loot_target&quot;</substitution>
    </alias>
    <alias name="loot_done" perleval="0" enabled="1" ignorecase="0">
      <pattern>^loot_done$</pattern>
      <substitution>/\$world-&gt;distrigger(&quot;loot&quot;,&quot;loot_corpse&quot;)%;drop_start</substitution>
    </alias>
    <alias name="^drop_start$" perleval="0" enabled="1" ignorecase="0">
      <pattern>^drop_start$</pattern>
      <substitution>drop_valueless%;study_start</substitution>
    </alias>
    <alias name="recover_start" perleval="1" enabled="1" ignorecase="0">
      <pattern>^recover_start$</pattern>
      <substitution>$world-&gt;enatrigger(&quot;recover&quot;);$world-&gt;enatimer(&quot;recover&quot;);&quot;hp&quot;</substitution>
    </alias>
    <alias name="recover_done" perleval="0" enabled="1" ignorecase="0">
      <pattern>^recover_done$</pattern>
      <substitution>/\$world-&gt;distrigger(&quot;recover&quot;);\$world-&gt;distimer(&quot;recover&quot;);%;use_pot_start</substitution>
    </alias>
    <alias name="study_start" perleval="1" enabled="1" ignorecase="0">
      <pattern>^study_start$</pattern>
      <substitution>$world-&gt;enatrigger(&apos;study&apos;);our @study_what = map(&quot;$_&quot; . &quot;_book&quot;,@basic_skills,@adv_skills);&quot;study_next&quot;</substitution>
    </alias>
    <alias name="study_done" perleval="0" enabled="1" ignorecase="0">
      <pattern>^study_done$</pattern>
      <substitution>/\$world-&gt;distrigger(&quot;study&quot;)%;recover_start</substitution>
    </alias>
    <alias name="use_pot_start" perleval="0" enabled="1" ignorecase="0">
      <pattern>^use_pot_start$</pattern>
      <substitution>/\$world-&gt;enatrigger(&quot;use_pot&quot;)%;hp</substitution>
    </alias>
    <alias name="start_practice" perleval="0" enabled="1" ignorecase="0">
      <pattern>^start_practice$</pattern>
      <substitution>/if(\$pot) {foreach(1..(\$pot \/ 10 \/ scalar(\@skills))){\$world-&gt;sendnoecho(map(&quot;lian \$_&quot;,\@skills))}}</substitution>
    </alias>
    <alias name="use_pot_done" perleval="0" enabled="1" ignorecase="0">
      <pattern>^use_pot_done$</pattern>
      <substitution>/\$world-&gt;distrigger(&quot;use_pot&quot;)%;next_round</substitution>
    </alias>
    <alias name="study_do" perleval="1" enabled="1" ignorecase="0">
      <pattern>^study_do$</pattern>
      <substitution>&quot;$study_what&quot; ? &quot;study $study_what&quot; : &quot;study_done&quot;</substitution>
    </alias>
    <alias name="study_next" perleval="1" enabled="1" ignorecase="0">
      <pattern>^study_next$</pattern>
      <substitution>$study_what = shift @study_what || &quot;&quot;;&quot;study_do&quot;</substitution>
    </alias>
    <alias name="study_full" perleval="1" enabled="1" ignorecase="0">
      <pattern>^study_full$</pattern>
      <substitution>&quot;tear $study_what%;study_do&quot;</substitution>
    </alias>
    <alias name="kill_reaction" perleval="0" enabled="1" ignorecase="0">
      <pattern>^kill_reaction$</pattern>
      <substitution>/\$world-&gt;distrigger(&quot;kill&quot;);send_alias(&quot;kill_reaction&quot;,\@kill_reaction);delay(3,&quot;/\$world-&gt;enatrigger(&apos;kill&apos;)&quot;)</substitution>
    </alias>
    <alias name="loot_room" perleval="0" enabled="1" ignorecase="0">
      <pattern>^loot_room$</pattern>
      <substitution>/our (\$loot_env,\$loot_target)=(&quot;&quot;,0);delay(3,&quot;loot_corpse&quot;);%;look</substitution>
    </alias>
    <alias name="loot_do" perleval="0" enabled="1" ignorecase="0">
      <pattern>^loot_do$</pattern>
      <substitution>/lootx(\$loot_item,\$loot_env)</substitution>
    </alias>
    <alias name="Common::repeat" perleval="0" enabled="1" ignorecase="0">
      <pattern>^#(\d+)\s+(.*)</pattern>
      <substitution>/repeat $1,&quot;$2&quot;</substitution>
    </alias>
    <alias name="Common::quicksend" perleval="0" enabled="1" ignorecase="0">
      <pattern>^#qs\s+(.+)$</pattern>
      <substitution>/qs &quot;$1&quot;</substitution>
    </alias>
    <alias name="Common::loot" perleval="0" enabled="1" ignorecase="0">
      <pattern>^loot(\s*|\s+[^\s]+\s*)$</pattern>
      <substitution>/loot(&quot;$1&quot;)</substitution>
    </alias>
  </aliases>
