<?xml version="1.0"?>
<anjuta>
    <plugin name="API Help" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-devhelp:AnjutaDevhelp"/>
    </plugin>
    <plugin name="Class Inheritance" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-class-inheritance:AnjutaClassInheritance"/>
    </plugin>
    <plugin name="Symbol Browser" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-symbol-browser:SymbolBrowserPlugin"/>
    </plugin>
    <plugin name="Task Manager" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-gtodo:GTodoPlugin"/>
    </plugin>
</anjuta>
